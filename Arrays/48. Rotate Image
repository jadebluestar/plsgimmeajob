class Solution:
    def rotate(self, matrix: List[List[int]]) -> None:
        """
        Do not return anything, modify matrix in-place instead.
        """
        n = len(matrix)
        
        og = [row[:] for row in matrix] # since python is list of lists that is why we have to make a copy of all the rows(lists) in the matrix(list)

        for row in range(n):
            for col in range(n):

                matrix[row][col] = og[n-1-col][row] # uses reverse mapping ie new[0][0] <- old[2][0]
