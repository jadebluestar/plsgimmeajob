class Solution:
    def majorityElement(self, nums: List[int]) -> List[int]:
        n = len(nums)
        res = []
        freq = {}

        for num in nums:

            freq[num] = freq.get(num, 0) + 1 #  if num in freq: basically means this
                                             #      freq[num] += 1
                                             #   else:
                                             #      freq[num] = 1


        for key in freq:
            if freq[key] > n//3: # threshold = n//3
                res.append(key)
        
        return res
